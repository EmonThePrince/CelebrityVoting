# Environment Configuration Template
# Copy this content to your .env file

# Server Configuration
PORT=5002
NODE_ENV=development

# Database Configuration - Update with your actual PostgreSQL credentials
# Format: postgresql://username:password@localhost:5432/database_name
DATABASE_URL=postgresql://your_username:your_password@localhost:5432/celebrity_voting_db

# Session Configuration - Generate a strong random secret
SESSION_SECRET=generate-a-strong-random-secret-here-minimum-32-characters

# API Configuration
VITE_API_BASE_URL=http://localhost:5002/api

# File Upload Configuration
MAX_FILE_SIZE=10485760
UPLOAD_PATH=./uploads
